<Window x:Class="Cuentame.VistasModelos.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="350" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid>
        <Label x:Name="label1" Content="Rut / Correo" HorizontalAlignment="Left" Margin="40,50,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="RutoEmail" HorizontalAlignment="Left" Height="26" Margin="150,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="190" TextAlignment="Center"/>
        <Label x:Name="label2" Content="Contraseña" HorizontalAlignment="Left" Margin="40,100,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="Pass" HorizontalAlignment="Left" Height="26" Margin="150,100,0,0" VerticalAlignment="Top" Width="190" />
        <Button Content="Button" HorizontalAlignment="Left" Margin="162,220,0,0" VerticalAlignment="Top" Width="90"/>

    </Grid>
</Window>
